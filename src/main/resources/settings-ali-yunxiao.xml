            </activation>
            <properties>
                <!-- 在 Windows 上设置本地仓库路径 -->
                <localRepository>D:/program/Java/repository/repo</localRepository>
            </properties>
        </profile>
        <profile>
            <id>linux-local-repo</id>
            <activation>
                <os>
                    <family>Linux</family>
                </os>
            </activation>
            <properties>
                <!-- 在 Linux 上设置本地仓库路径 -->
                <localRepository>/home/user/.m2/repository</localRepository>
            </properties>
        </profile>
        <profile>
